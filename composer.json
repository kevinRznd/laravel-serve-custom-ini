    "license": "MIT",
    "version": "0.1",
    "keywords": ["laravel", "laravel5", "serve", "xdebug", "cli"],
    "authors": [
        {
            "name": "MichaÅ‚ Mieluch",
            "email": "michal.mieluch@bluefroglondon.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mmieluch\\LaravelServeCustomIni\\": "src"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.x|5.1.x|5.2.x",
        "illuminate/console": "5.0.x|5.1.x|5.2.x"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
